<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="linuxcncserver" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug-Linux">
				<Option platforms="Unix;" />
				<Option output="../../../bin/plugins/i386-linux-gnu/linuxcncserver/cncremote-linuxcnc" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-O0" />
					<Add option="-DULAPI" />
					<Add option="-D_LINUX" />
					<Add directory="$(LINUXCNC)/src/emc/usr_intf" />
					<Add directory="$(LINUXCNC)/include" />
					<Add directory="../../../include" />
					<Add directory="../../../src" />
					<Add directory="../../../libraries/build/include" />
				</Compiler>
				<Linker>
					<Add library="linuxcnchal" />
					<Add library="linuxcnc" />
					<Add library="nml" />
					<Add library="liblinear.a" />
					<Add library="libuv.a" />
					<Add library="libtv.a" />
					<Add library="pthread" />
					<Add library="linuxcncini" />
					<Add directory="$(LINUXCNC)/lib" />
					<Add directory="../../../libraries/build/lib" />
				</Linker>
				<ExtraCommands>
					<Add before="./version.sh $(LINUXCNC)" />
				</ExtraCommands>
			</Target>
			<Target title="Release-Linux">
				<Option platforms="Unix;" />
				<Option output="../../../bin/plugins/i386-linux-gnu/linuxcncserver/cncremote-linuxcnc" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-g" />
					<Add option="-O0" />
					<Add option="--std=c++11" />
					<Add option="-DULAPI" />
					<Add option="-D_LINUX" />
					<Add directory="$(LINUXCNC)/src/emc/usr_intf" />
					<Add directory="$(LINUXCNC)/include" />
					<Add directory="../../../include" />
					<Add directory="../../../src" />
					<Add directory="../../../libraries/build/include" />
				</Compiler>
				<Linker>
					<Add library="linuxcnchal" />
					<Add library="linuxcnc" />
					<Add library="nml" />
					<Add library="liblinear.a" />
					<Add library="libuv.a" />
					<Add library="libtv.a" />
					<Add library="pthread" />
					<Add library="linuxcncini" />
					<Add directory="$(LINUXCNC)/lib" />
					<Add directory="../../../libraries/build/lib" />
				</Linker>
				<ExtraCommands>
					<Add before="./version.sh $(LINUXCNC)" />
					<Add after="./copy.sh $(LINUXCNC) $(TARGET_OUTPUT_FILE)" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
